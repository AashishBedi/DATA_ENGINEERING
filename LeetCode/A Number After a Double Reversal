class Solution:
    def isSameAfterReversals(self, num: int) -> bool:
        s = str(num)
        s = s[::-1]
        s = int(s)
        s = str(s)
        return int(s[::-1]) == num

#Way 2
class Solution:
    def isSameAfterReversals(self, num: int) -> bool:
        if num%10 == 0 and num != 0:
            return False
        return True
