class Solution:
    def subsets(self, nums: List[int]) -> List[List[int]]:
        res = []

        SubSet = []
        def dfs(i):
            if i>=len(nums):
                res.append(SubSet.copy())
                return 
            
            #decision to include nums[i]
            SubSet.append(nums[i])
            dfs(i+1)

            #decision to not include nums[i]
            SubSet.pop()
            dfs(i+1)

        dfs(0)
        return res
