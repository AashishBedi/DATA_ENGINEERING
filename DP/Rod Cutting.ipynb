   "source": [
    "class Solution:\n",
    "    def cutRod(self, price, N):\n",
    "        dp = [0]*(N+1) \n",
    "        \n",
    "        for rl in range(1, N+1):\n",
    "            ans = 0\n",
    "            for cl in range(1, rl+1):\n",
    "                ans = max(ans, price[cl-1]+dp[rl-cl])\n",
    "            dp[rl] = ans\n",
    "        return dp[N]"
   ]
  }
